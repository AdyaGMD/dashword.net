    <!-- https://stackoverflow.com/a/20505898 -->
    <script src="/assets/js/ads-prebid.js"></script>

    <!-- Sitemap -->
    <link rel="sitemap" type="application/xml" title="Sitemap" href="/sitemap.xml">

    {% if pageID == "authorPosts" %}{% set myAuthor = authors | getAuthor(category.title) %}{% endif %}
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {% if page404 %}
    <!-- If 404 -->
    <meta name="robots" content="noindex,follow">
    {% endif %}

    <meta name="theme-color" content="#0067EE">
    <link rel="icon" href="/assets/img/icon.png">

    <!-- Metadata -->
    <title>
    {% if pageID !== "authorPosts" %}{{ title or metadata.title }}{% else %}{{ myAuthor.name }}{% endif %} | {{ metadata.title }}
    </title>
    <meta name="title" content="{% if pageID !== "authorPosts" %}{{ title or metadata.title }}{% else %}{{ myAuthor.name }}{% endif %} | {{ metadata.title }}">
    <meta name="description" content="{% if pageID !== "authorPosts" %}{{ desc or metadata.desc }}{% else %}{{ myAuthor.bio }}{% endif %}">
    {% if date %}<meta name="date" content="{{ page.date | readableDate }}">{% endif %}

    <!-- MAIN CSS -->
    <link rel="stylesheet" href="/assets/css/index.css">

    <!-- ICON CSS -->
    <link href="https://cdn.jsdelivr.net/npm/remixicon@2.5.0/fonts/remixicon.css" rel="stylesheet">

    <!-- Toastify JS -->
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/toastify-js/src/toastify.min.css">

    {% if hasModal %}
    <!-- Tingle -->
    <link rel="stylesheet" type="text/css" href="/assets/css/tingle.css">
    {% endif %}

    <!-- Opengraph -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="{{ metadata.url }}">
    <meta property="og:title" content="{% if pageID !== "authorPosts" %}{{ title or metadata.title }}{% else %}{{ myAuthor.name }}{% endif %} | {{ metadata.title }}">
    <meta property="og:description" content="{% if pageID !== "authorPosts" %}{{ desc or metadata.desc }}{% else %}{{ myAuthor.bio }}{% endif %}">
    <meta property="og:image" content="{{ image or metadata.image }}">

    <!-- Twitter since they're different or something -->
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:url" content="{{ metadata.url }}">
    <meta property="twitter:title" content="{% if pageID !== "authorPosts" %}{{ title or metadata.title }}{% else %}{{ myAuthor.name }}{% endif %} | {{ metadata.title }}">
    <meta property="twitter:description" content="{% if pageID !== "authorPosts" %}{{ desc or metadata.desc }}{% else %}{{ myAuthor.bio }}{% endif %}">
    <meta property="twitter:image" content="{{ image or metadata.image }}">

    <!-- Netlify CMS -->
    <script src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>

</head>
<body>

    <!-- NOTIF ALERT START -->
    <div class="notif cols" id="notif">
        <div class="notif-content col">
            <p>Dashword uses the revenue generated from ads to cover its costs. You are free to keep your ad blocker on, but if you want to help us continue to share high quality Geometry Dash news, we comfortably ask you to consider disabling your ad blocker on this website.</p>
        </div>
        <div>
            <button class="button" onclick="closeNotif()">Close</button>
        </div>
    </div>
    <!-- NOTIF ALERT END -->

    {% if metadata.isAds and not hideAds %}
    <!-- FIXED SLOT START -->
    <div class="fixed-slot">
        <div>
            {% include 'formats/display-wide.njk'%}
        </div>
    </div>
    <!-- FIXED SLOT END -->
    {% endif %}

    <!-- SCROLL LINK START -->
    <div class="scroll" id="scrollTop" onclick="up()" onkeydown="if (event.keyCode === 13) {up()}" tabindex="0">
        <i class="ri-arrow-up-line"></i>
    </div>
    <!-- SCROLL LINK END -->

    <!-- MOBILE NAV START -->
    <div class="mobile" id="menu">
        <i class="ri-close-fill close" id="close" onclick="closeMobile()" onkeydown="if (event.keyCode === 13) {closeMobile()}" tabindex="0"></i>
        <div style="display: flex; flex-direction: column; height: 100%;">
            <a href="/" class="mobile-item">Home</a>
            <a href="/categories/2.2/" class="mobile-item">2.2</a>
            <a href="/categories/news/" class="mobile-item">News</a>
            <a href="/categories/demonlist/" class="mobile-item">Demonlist</a>
            <a href="/categories/guide/" class="mobile-item">Guide</a>
            <a href="/categories/opinion/" class="mobile-item">Opinion</a>
            <a href="/categories/review/" class="mobile-item">Review</a>
            <a href="/search/" class="mobile-item" style="margin-top: auto; display: flex; align-items: center; gap: 5px;"><i class="ri-search-line"></i> Search</a>
        </div>
    </div>
    <div class="mobile-bg" id="menu-bg" onclick="closeMobile()"></div>
    <!-- MOBILE NAV END -->

